{
  "name": "mytheme-blocks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "checkFormat": "prettier --check \"**/*.{js,json,css,scss}\"",
    "format": "prettier --write \"**/*.{js,json,css,scss}\"",
    "lint": "eslint src",
    "start": "webpack --mode=development --watch",
    "build": "npm run lint && webpack --mode=production && gulp bundle",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "**/*.{js,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@wordpress/blocks": "^7.0.6",
    "@wordpress/i18n": "^3.18.0",
    "jquery": "^3.6.0",
    "node-sass": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@wordpress/block-editor": "^5.2.10",
    "@wordpress/browserslist-config": "^3.0.1",
    "@wordpress/components": "^12.0.8",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.22.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
